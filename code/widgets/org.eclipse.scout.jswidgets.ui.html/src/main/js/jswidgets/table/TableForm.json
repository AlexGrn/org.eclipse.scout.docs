                  {
                    "id": "AutoResizeColumnsField",
                    "objectType": "CheckBoxField",
                    "label": "Auto Resize Columns",
                    "tooltipText": "${textKey:AutoResizeColumnsTooltip}",
                    "labelVisible": false,
                    "gridDataHints": {
                      "fillHorizontal": false
                    }
                  },
                  {
                    "id": "AutoOptimizeColumnWidthsField",
