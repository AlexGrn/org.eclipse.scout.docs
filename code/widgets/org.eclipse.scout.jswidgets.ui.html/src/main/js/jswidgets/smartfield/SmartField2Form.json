{
  "id": "jswidgets.SmartField2Form",
  "type": "model",
  "displayHint": "view",
  "rootGroupBox": {
    "id": "MainBox",
    "objectType": "GroupBox",
    "fields": [
      {
        "id": "TableSmartFieldBox",
        "objectType": "GroupBox",
        "gridColumnCount": 1,
        "label": "SmartField with table proposal",
        "fields": [
           {
            "id": "TableSmartField",
            "objectType": "SmartField2",
            "lookupCall": "jswidgets.LocaleLookupCall",
            "label": "${textKey:Locale}"
           },
           {
            "id": "ValueField",
            "objectType": "StringField",
            "label": "Smartfield value"
           },
           {
             "id": "SetValueButton",
             "objectType": "Button",
             "label": "Set value"
           }
        ]
      },
      {
        "id": "TreeSmartFieldBox",
        "objectType": "GroupBox",
        "gridColumnCount": 1,
        "label": "Smart field with tree proposal",
        "fields": [
           {
            "id": "TreeSmartField",
            "objectType": "SmartField2",
            "lookupCall": "jswidgets.WorldLookupCall",
            "label": "${textKey:WorldRegion}",
            "browseHierarchy": true
           },
           {
             "id": "ExpandTreeNodesField",
             "objectType": "CheckBoxField",
             "label": "${textKey:ExpandTreeNodes}"
           },
           {
             "id": "LoadIncrementalField",
             "objectType": "CheckBoxField",
             "label": "${textKey:LoadIncremental}"
           }
        ]
      },
      {
        "id": "ProposalFieldBox",
        "objectType": "GroupBox",
        "gridColumnCount": 1,
        "label": "Proposal field",
        "fields": [
           {
            "id": "ProposalField",
            "objectType": "ProposalField2",
            "lookupCall": "jswidgets.SalutationLookupCall",
            "label": "${textKey:Salutation}"
           }
        ]
      },
      {
        "id": "DropdownFieldBox",
        "objectType": "GroupBox",
        "gridColumnCount": 1,
        "label": "Dropdown field",
        "fields": [
           {
            "id": "DropdownField",
            "objectType": "SmartField2",
            "lookupCall": "jswidgets.SalutationLookupCall",
            "label": "${textKey:Salutation}",
            "displayStyle": "dropdown"
           }
        ]
      }
    ]
  }
}
