{
  "id": "jswidgets.TableFieldForm",
  "objectType": "Form",
  "type": "model",
  "displayHint": "view",
  "rootGroupBox": {
    "id": "MainBox",
    "mainBox": true,
    "objectType": "GroupBox",
    "fields": [
      {
        "id": "DetailBox",
        "objectType": "GroupBox",
        "gridColumnCount": 1,
        "fields": [
          {
            "id": "TableField",
            "objectType": "TableField",
            "labelVisible": false,
            "gridDataHints": {
              "h": 2
            },
            "table": {
              "id": "Table",
              "objectType": "Table",
              "columns": [
                {
                  "index": 0,
                  "text": "${textKey:RowName}",
                  "id": "RowName",
                  "width": 300,
                  "objectType": "Column"
                },
                {
                  "index": 1,
                  "text": "${textKey:GroupNo}",
                  "id": "GroupNo",
                  "width": 175,
                  "objectType": "NumberColumn"
                },
                {
                  "index": 2,
                  "text": "${textKey:RowNo}",
                  "id": "RowNo",
                  "width": 175,
                  "objectType": "NumberColumn"
                }
              ],
              "tableControls": [
                {
                  "id": "AggregateTableControl",
                  "objectType": "AggregateTableControl",
                  "iconId": "${iconId:SUM}",
                  "tooltipText": "${textKey:ui.Total}"
                }
              ],
              "menus": [
                {
                  "id": "AddRowsMenu",
                  "objectType": "Menu",
                  "text": "${textKey:AddRows}",
                  "menuTypes": ["Table.EmptySpace"]
                },
                {
                  "id": "ColumnVisibilityMenu",
                  "objectType": "Menu",
                  "text": "${textKey:ColumnVisibility}",
                  "menuTypes": ["Table.EmptySpace"]
                },
                {
                  "id": "GroupingStyle",
                  "objectType": "Menu",
                  "text": "${textKey:GroupingStyle}",
                  "menuTypes": ["Table.EmptySpace"],
                  "childActions": [
                    {
                      "id": "GroupingStyleTop",
                      "objectType": "Menu",
                      "text": "${textKey:GroupingStyleTop}",
                      "menuTypes": ["Table.EmptySpace"]
                    },
                    {
                      "id": "GroupingStyleBottom",
                      "objectType": "Menu",
                      "text": "${textKey:GroupingStyleBottom}",
                      "menuTypes": ["Table.EmptySpace"]
                    }
                  ]
                }
              ]
            }
          }
        ]
      }
    ]
  }
}
